{
	"name": "regexp-syntax-highlighter",
	"displayName": "Injected Regex Syntax Highlighting",
	"description": "Syntax Highlighting for regex's in other languages",
	"publisher": "RedCMD",
	"version": "1.1.1",
	"engines": {
		"vscode": "^1.47.0"
	},
	"icon": "images/icon.png",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RedCMD/regex-syntax-highlighter-vscode.git"
	},
	"bugs": {
		"url": "https://github.com/RedCMD/JSON-Embedded-Languages/issues"
	},
	"keywords": [
		"regex",
		"typescript",
		"javascript"
	],
	"categories": [
		"Programming Languages"
	],
	"scripts": {
		"package": "vsce package",
		"publish": "vsce publish",
		"test": "vscode-tmgrammar-snap ./syntaxes/tests/*.js --scope source.js --grammar ./syntaxes/tests/JavaScript.tmLanguage.json",
		"test:update": "npm run test -- --updateSnapshot"
	},
	"contributes": {
		"languages": [
			{
				"id": "regexp",
				"aliases": [
					"regexp"
				],
				"extensions": [
					".js.regexp",
					".ts.regexp"
				],
				"icon": {
					"dark": "./images/icon-dark.svg",
					"light": "./images/icon-light.svg"
				},
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "regexp",
				"scopeName": "source.js.regexp",
				"path": "./syntaxes/regexp-js.tmLanguage.json",
				"embeddedLanguages": {
					"meta.embedded.js.regexp": "regexp"
				},
				"injectTo": [
					"source.js",
					"source.js.jsx",
					"source.ts",
					"source.tsx"
				]
			}
		]
	},
	"devDependencies": {
		"vsce": "^2.15.0",
		"vscode-tmgrammar-test": "^0.1.3"
	}
}
